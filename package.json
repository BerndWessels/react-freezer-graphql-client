{
  "name": "react-freezer-graphql-client",
  "version": "0.1.0",
  "description": "A react app using freezer and graphql without relay!",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "build:serve": "babel-node --eval \"var script = require('./scripts/buildServe'); script.default().catch(err => console.error(err.stack))\"",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress --hot --watch",
    "externals:update": "babel-node --eval \"var script = require('./scripts/externalsUpdate'); script.default().catch(err => console.error(err.stack))\"",
    "postinstall": "npm run externals:update",
    "start": "npm run dev",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "translations:extract": "babel-node --eval \"var script = require('./scripts/translationsExtract'); script.default().catch(err => console.error(err.stack))\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "author": "Bernd Wessels <wesselsbernd@gmail.com> (https://github.com/BerndWessels)",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.0.0",
    "freezer-js": "^0.11.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-intl": "^2.0.0",
    "react-router": "^2.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.0",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-react-intl": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.0.0",
    "browser-sync": "^2.0.0",
    "browser-sync-webpack-plugin": "^1.0.0",
    "chai": "^3.0.0",
    "clean-webpack-plugin": "^0.1.0",
    "copy-webpack-plugin": "^2.0.0",
    "cors": "^2.0.0",
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.0",
    "express": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "faker": "^3.0.0",
    "file-loader": "^0.8.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.0.0",
    "imports-loader": "^0.6.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jsdom": "^8.0.0",
    "json-loader": "^0.5.0",
    "less": "^2.0.0",
    "less-loader": "^2.0.0",
    "mocha": "^2.0.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.0.0",
    "null-loader": "^0.1.0",
    "postcss-loader": "^0.9.0",
    "q": "^1.0.0",
    "raw-loader": "^0.5.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.0",
    "resolve-url-loader": "^1.0.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.0.0",
    "webpack-dev-server": "^1.0.0"
  }
}
